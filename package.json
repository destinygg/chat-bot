    "google-auth-library": "^6.1.1",
    "googleapis": "^118.0.0",
    "inspector-influx": "^2.7.0",
    "inspector-metrics": "^1.21.0",
    "lodash": "^4.17.20",
    "metascraper": "^5.14.20",
    "metascraper-date": "^5.14.20",
    "moment": "^2.29.1",
    "moment-timezone": "^0.5.31",
    "protobufjs": "^6.10.1",
    "sqlite3": "^5.0.0",
    "twitch-js": "^2.0.0-beta.45",
    "twitter": "^1.1.0",
